{% extends 'base.html.twig' %}

{% block page_header %}
    <div class="page-head">
        <h2>{{ page_header }}</h2>
        <ol class="breadcrumb">
            {% if app.user.isRadioStationCompiler or app.user.isRadioStationAdmin %}
                <li><a href="{{ path('radio_station_profile',{slug:app.user.radioStation.slug}) }}">Home</a></li>
            {% else %}
                <li><a href="{{ path('dashboard_home') }}">Home</a></li>
            {% endif %}
            <li><a href="{{ path('radio_station_list') }}">Radio stations</a></li>
            {% if action == 'radio_station_song_reject' or action == 'radio_station_song_reject_view'%}
                <li><a href="{{ path('radio_station_incoming_list') }}">Incoming</a></li>
            {% endif %}
            <li class="active">{{ breadcrumb }}</li>
        </ol>
    </div>
{% endblock page_header %}

{% block body %}

{% endblock body %}