<form class="form-horizontal group-border-dashed" action="{{ path('radio_show_type_add') }}" data-parsley-validate style="border-radius: 0px;"  autocomplete="off" {% if app.debug %}  novalidate="true"{% endif %}  method="POST" enctype="multipart/form-data">
    <div class="col-sm-12 col-md-12">
        <fieldset>
            <legend><i class="fa fa-info-circle-circle"></i>&nbsp;Title</legend>
            <div class="row">
                <div class="col-sm-6">
                    {{ form_row(form.title) }}
                </div>
            </div>
        </fieldset>
    </div>
    <div class="col-sm-12 col-md-12">
        <div class="text-center">
            <button class="btn btn-danger btn-lg" type="submit" tabindex="2"><i class="fa fa-save"></i> Add</button>
            <a href="{{ path('radio_show_list') }}" class="btn btn-default" tabindex="3" title="Cancel" alt="Cancel">Cancel</a>
        </div>
    </div>
    <div class="form-group"></div>
    {{ form_rest(form) }}
</form>


