<form class="form-horizontal group-border-dashed" action="{{ path('advertising_organization_add') }}" data-parsley-validate style="border-radius: 0px;"  autocomplete="off" {% if app.debug %}  novalidate="true"{% endif %}  method="POST" enctype="multipart/form-data">
    <div class="col-sm-12 col-md-12">
        <fieldset>
            <legend><i class="fa fa-info-circle"></i>&nbsp;Organization details</legend>
            <div class="row">
                <div class="col-sm-6">
                    {{ form_row(form.name) }}
                </div>
                <div class="col-sm-6">
                    {{ form_row(form.industry) }}
                </div>
            </div>
        </fieldset>
        <fieldset>
            <legend><i class="fa fa-phone-square"></i>&nbspContact details</legend>
            <div class="row">
                <div class="col-sm-6">
                    {{ form_row(form.contact) }}
                </div>
                <div class="col-sm-6">
                    {{ form_row(form.email) }}
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    {{ form_row(form.telephone) }}
                </div>
            </div>
        </fieldset>
        <fieldset>
            <legend><i class="fa "></i>&nbsp;Meta data</legend>
            <div class="row">
                <div class="col-sm-6">
                    {{ form_row(form.createdAt) }}
                </div>
                <div class="col-sm-6">
                    {{ form_row(form.updatedAt) }}
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    {{ form_row(form.lastActiveAt) }}
                </div>
            </div>
        </fieldset>
    </div>
    <div class="form-group"></div>
    {{ form_rest(form) }}
</form>


